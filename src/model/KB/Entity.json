{
    "@context": {
        "skos": "https://www.w3.org/TR/skos-reference/",
        "property": "https://wiki-dev.open-semantic-lab.org/id/Property-3A",
        "label_": "skos:prefLabel",
        "label__": "property:HasLabel",
        "test": "property:HasTestProperty"
    },
    "title": "Entity",
    "type": "object",
    "id": "entity",
    "required": [
        "osl_template",
        "osl_wikitext",
        "osl_footer",
        "uuid",
        "label"
    ],
    "properties": {
        "osl_template": {
            "type": "string",
            "enum": [
                "OslTemplate:KB/Entity"
            ],
            "default": "OslTemplate:KB/Entity",
            "options": {
                "hidden": true
            }
        },
        "uuid": {
            "title": "UUID",
            "type": "string",
            "format": "uuid",
            "options": {
                "hidden": true
            }
        },
        "label": {
            "title": "Label",
            "description": "Human readable name",
            "$ref": "../Label.json"
        },
        "additional_labels": {
            "type": "array",
            "title": "Additional Labels",
            "format": "table",
            "items": {
                "$ref": "../Label.json"
            }
        },
        "statements": {
            "type": "array",
            "title": "Statements",
            "format": "tabs",
            "items": {
                "$ref": "../Statement.json"
            }
        },
        "extensions": {
            "title": "Extensions",
            "type": "array",
            "format": "tabs",
            "items": {
                "title": "Extension",
                "headerTemplate": "{{i1}} - {{self.osl_template_label}}",
                "options": {
                    "keep_oneof_values": false
                },
                "oneOf": []
            }
        },
        "osl_wikitext": {
            "type": "string",
            "format": "markdown",
            "options": {
                "hidden": true
            },
            "default": "\n=Details=\n"
        },
        "osl_footer": {
            "type": "object",
            "id": "footer",
            "title": "KB/Entity/Footer",
            "options": {
                "hidden": true
            },
            "required": [
                "osl_template"
            ],
            "properties": {
                "osl_template": {
                    "type": "string",
                    "default": "OslTemplate:KB/Entity/Footer",
                    "options": {
                        "hidden": true
                    }
                }
            },
            "default": {
                "osl_template": "OslTemplate:KB/Entity/Footer"
            }
        }
    }
}